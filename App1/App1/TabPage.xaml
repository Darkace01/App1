<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="App1.TabPage"
             xmlns:local="clr-namespace:App1">
    <NavigationPage Title="Contacts" Icon="edge.png">
        <x:Arguments>
            <local:SearchPage />
        </x:Arguments>
    </NavigationPage>
    <ContentPage Title="Page1" Icon="chrome.png">
        <Label Text="Content Of Page One" HorizontalOptions="Center" VerticalOptions="Center" FontSize="Large"></Label>
    </ContentPage>
    <ContentPage Title="Page2" Icon="edge.png">
        <Label Text="Content Of Page Two" HorizontalOptions="Center" VerticalOptions="Center" FontSize="Large"></Label>
    </ContentPage>
</TabbedPage>